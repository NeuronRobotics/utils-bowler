JAVA_HOME=/usr/lib/jvm/default-java

%:
    dh $@ --with javahelper

override_dh_link:
    dh_link usr/local/NeuronRobotics/RDK/bowlerstudio usr/bin/bowlerstudio

override_dh_auto_build:
    dh_auto_build -- dist

override_dh_install:
    dh_auto_build -- package-debian
    dh_install

override_jh_exec:
    exit 0
